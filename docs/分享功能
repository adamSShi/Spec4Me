// åˆ†äº«é‚è¼¯
function handleShare() {
  if (!props.layer || !props.itemId) return;
  
  // ç”Ÿæˆåˆ†äº«é€£çµ
  const shareUrl = generateShareLink({
    layer: props.layer,
    id: props.itemId,
  });
  
  // é–‹å•Ÿåˆ†äº«å°è©±æ¡†
  showShareDialog.value = true;
}
