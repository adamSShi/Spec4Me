  title: string;         // åˆ†äº«æ¨™é¡Œ
  address?: string;      // åœ°å€/æè¿°
  shareUrl: string;      // åˆ†äº«é€£çµ
  thumbnail?: string;    // ç¸®åœ– URL (è¡—æ™¯åœ–ç‰‡)
}

// Emits
interface ShareDialogEmits {
  'update:modelValue': (value: boolean) => void;
  'close': () => void;
}

// åŠŸèƒ½
- é¡¯ç¤ºåˆ†äº«è³‡è¨Š (æ¨™é¡Œã€åœ°å€ã€ç¸®åœ–)
- é¡¯ç¤ºåˆ†äº«é€£çµ
- è¤‡è£½é€£çµåˆ°å‰ªè²¼ç°¿
- æ‰‹æ©Ÿç‰ˆï¼šè§¸ç™¼åŸç”Ÿåˆ†äº« (navigator.share)
```

### 5.3 shareInfo Feature

**æª”æ¡ˆä½ç½®**: `src/components/features/shareInfo.vue`

```typescript
// åŠŸèƒ½
- è§£æ URL åˆ†äº«åƒæ•¸
- æ ¹æ“šåˆ†äº«é¡å‹æŸ¥è©¢å°æ‡‰è³‡æ–™
- é¡¯ç¤ºåˆ†äº«é …ç›®è³‡è¨Š (InfoItemCard)
- åœ¨åœ°åœ–ä¸Šæ¨™è¨˜ä½ç½®
- ç§»å‹•åœ°åœ–è¦–è§’åˆ°ç›®æ¨™ä½ç½®

